#' @export
prepare_raw_key_value_4_db <- function(filepath) {
df <- read_csv(filepath, name_repair = name_repair)
(
df
|> rename(
champ = Champ,
statut = Statut,
annee = Annee,
periode = Period,
ipe = IPE
)
) -> df
champ   <- pull(df[1, ], champ  )
statut  <- pull(df[1, ], statut )
annee   <- pull(df[1, ], annee  )
periode <- pull(df[1, ], periode)
ipe     <- pull(df[1, ], ipe    )
}
